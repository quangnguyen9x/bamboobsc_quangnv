<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="BbTsaMaCoefficients">

	<resultMap type="com.netsteadfast.greenstep.po.hbm.BbTsa" id="baseResultMap">
		<id column="OID" property="oid"/>
		<result column="TSA_OID" property="tsaOid"/>
		<result column="SEQ" property="seq"/>
		<result column="SEQ_VALUE" property="seqValue"/>
		<result column="CUSERID" property="cuserid"/>
		<result column="CDATE" property="cdate"/>
		<result column="UUSERID" property="uuserid"/>
		<result column="UDATE" property="udate"/>
	</resultMap>
	
	<select id="selectByParams" resultMap="baseResultMap" >
		select * from bb_tsa_ma_coefficients where 1=1
		<if test="oid != null">
			AND OID = #{oid}
		</if>
		<if test="tsaOid != null">
			AND TSA_OID = #{tsaOid}
		</if>		
	</select>
	
	<select id="selectByValue" resultMap="baseResultMap" >
		select * from bb_tsa_ma_coefficients where 1=1
		<if test="oid != null">
			AND OID = #{oid}
		</if>
		<if test="tsaOid != null">
			AND TSA_OID = #{tsaOid}
		</if>			
	</select>	
	
</mapper>